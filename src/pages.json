{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "LifaBox",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#707070",
    "selectedColor": "#8433e2",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home_active.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/my.png",
        "selectedIconPath": "static/tabbar/my_active.png",
        "pagePath": "pages/profile/index",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home"
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "style": {
        "navigationBarTitleText": "关于"
      }
    },
    {
      "path": "pages/courses/add",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "新增课程"
      }
    },
    {
      "path": "pages/items/add",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "新增物品"
      }
    },
    {
      "path": "pages/items/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "物品"
      }
    },
    {
      "path": "pages/learn/index",
      "type": "page"
    },
    {
      "path": "pages/profile/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "个人中心"
      }
    }
  ],
  "subPackages": []
}